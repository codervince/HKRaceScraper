<script type="text/javascript" language="javascript">
    function goURL(url) {
        // JCRWSystemCutoffDate, HkjcWebUrl, curIsEnglish, jsAction From Site.Master
        var cutOffDate = JCRWSystemCutoffDate;
        var curSelect = document.getElementById("raceDateSelect");
        var curArgs = curSelect.options[curSelect.selectedIndex].value;

        var curDate = curArgs.split("/")[1];
        var isSimulcastOnly = curArgs.indexOf("Simulcast") >= 0;

        if ((curDate < cutOffDate) && isSimulcastOnly) {
            var paraDate = curSelect.options[curSelect.selectedIndex].text;
            url = HkjcWebUrl + "/" + (curIsEnglish ? "english" : "chinese") + "/racing/";
            if (jsAction == "resultsall") {
                url += "raceresult_all.asp?raceDate=" + paraDate;
            }
            else {
                url += "results.asp?raceDate=" + paraDate + "&venue=" + curArgs.split("/")[2];
            }
            parent.document.location.href = url;
        }
        else {
            if (jsAction == "resultsall")
                curArgs = curArgs.replace("Simulcast", "Local");
            url = url.replace("Local/yyyyMMdd/ST", curArgs);
            parent.document.location.href = url;
        }
    }
</script>


